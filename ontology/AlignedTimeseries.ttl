  rdfs:comment "The relation of a TimeseriesNodeSet to a procedure such as integrating a set of observations."@en ;
  rdfs:domain ats:TimeseriesNodeSet ;
  rdfs:range saref:ProcedureExecution ;
  rdfs:label "has procedure"@en .

### https://saref.etsi.org/saref4tser/hasMember
ats:hasMember a owl:ObjectProperty ;
  owl:inverseOf ats:isMemberOf ;
  rdfs:comment "The relation of an AlignedSequence to its set of Observations."@en ;
  rdfs:domain ats:AlignedSequence ;
  rdfs:range saref:Observation ;
  rdfs:label "has member"@en .

### https://saref.etsi.org/saref4tser/isMemberOf
ats:isMemberOf a owl:ObjectProperty ;
  rdfs:comment "The relationship between an Observation and the AlignedSequence to which it belongs."@en ;
  rdfs:domain saref:Observation ;
  rdfs:range ats:AlignedSequence ;
  owl:inverseOf ats:hasMember ;
  rdfs:label "is member of"@en .

### https://saref.etsi.org/saref4tser/hasEventBasedSamplingTime
ats:hasEventBasedSamplingTime a owl:ObjectProperty ;
  rdfs:comment "Declares that the AlignedSequence samples Observations based upon threshold events."@en ;
  rdfs:subPropertyOf ats:hasSamplingTime ;
  rdfs:domain ats:AlignedSequence ;
  rdfs:range ats:EventBasedTimeSeriesObservation ;
  rdfs:label "has event based sampling time"@en .

### https://saref.etsi.org/saref4tser/hasIntervalBasedSamplingTime
ats:hasIntervalBasedSamplingTime a owl:ObjectProperty ;
  rdfs:comment "Declares that the AlignedSequence samples Observations based upon a constant time interval."@en ;
  rdfs:subPropertyOf ats:hasSamplingTime ;
  rdfs:domain ats:AlignedSequence ;
  rdfs:range ats:IntervalBasedTimeSeriesObservation ;
  rdfs:label "has interval based sampling time"@en .

### https://saref.etsi.org/saref4tser/hasIdentifier
ats:hasIdentifier a owl:ObjectProperty ;
  rdfs:domain ats:Identifier ;
  rdfs:range ats:hasIdentifierValue ;
  rdfs:comment "Declares the identifier of an entity."@en ;
  rdfs:label "has identifier"@en .

### https://saref.etsi.org/saref4tser/hasRequestor
ats:hasRequestor a owl:ObjectProperty ;
  rdfs:domain ats:Identifier ;
  rdfs:range ats:hasRequestorValue ;
  rdfs:comment "Declares the requestor of a query or the owner of a project."@en ;
  rdfs:label "has requestor"@en .

### https://saref.etsi.org/saref4tser/hasStationarity
ats:hasStationarity a owl:ObjectProperty ;
  rdfs:domain ats:AlignedTimeseriesModel ; 
  rdfs:range ats:AlignedSequenceFunction ;
  rdfs:comment "A property indicating the stationarity of a AlignedTimeseriesModel {true or false}. The Augmented Dickey-Fuller (ADF) test or the Kwiatkowski-Phillips-Schmidt-Shin (KPSS) test reveals whether a time series is stationary or not. Stationarity implies a time series without trend or seasonal components."@en ;
  rdfs:label "has stationarity"@en .

#################################################################
#    Datatype Properties 
#################################################################

### https://saref.etsi.org/saref4tser/hasDataPointType
ats:hasDataPointType a owl:DatatypeProperty ;
  rdfs:domain saref:Observation ; 
  rdfs:range [
      a rdfs:Datatype ;
      owl:oneOf (
        "measured="^^xsd:string
        "imputed+"^^xsd:string
        "interpolated*"^^xsd:string
        "removed-"^^xsd:string
        "synthetic^"^^xsd:string
        "unknown?"^^xsd:string
      ) ;
  ] ;
  rdfs:comment "Declares the type of data point. The last character can be used as code."@en ;
  rdfs:label "has data point type"@en .
 
### http://www.w3.org/2006/time#inXSDDateTimeStamp
time:inXSDDateTimeStamp a owl:DatatypeProperty ;
  rdfs:domain time:Instant ;
  rdfs:range xsd:dateTimeStamp ;
  rdfs:comment "An instant expressed using xsd:dateTimeStamp" ;
  rdfs:isDefinedBy "http://www.w3.org/2006/time" ;
  rdfs:label "inXSDDateTimeStamp"@en .

### https://saref.etsi.org/saref4tser/hasSequenceDatatype
ats:hasSequenceDatatype a owl:DatatypeProperty ;
  rdfs:domain ats:AlignedSequence ; 
  rdfs:range [
      a rdfs:Datatype ;
      owl:oneOf (
        "string"^^xsd:string
        "integer"^^xsd:string
        "int32"^^xsd:string
        "int64"^^xsd:string
        "long"^^xsd:string
        "short"^^xsd:string
        "byte"^^xsd:string
        "unsignedLong"^^xsd:string
        "unsignedInt"^^xsd:string
        "boolean"^^xsd:string
        "decimal"^^xsd:string
        "float"^^xsd:string
        "double"^^xsd:string
        "duration"^^xsd:string
        "date"^^xsd:string
        "time"^^xsd:string
        "dateTime"^^xsd:string
      ) ;
  ] ;
  rdfs:comment "Declares the type of data of a sequence of time-indexed values."@en ;
  rdfs:label "has sequence data type"@en .
 
### https://saref.etsi.org/saref4tser/hasIdentifierValue
ats:hasIdentifierValue a owl:DatatypeProperty ;
  rdfs:domain ats:hasIdentifier ; 
  rdfs:range xsd:string ;
  rdfs:comment "A relationship that defines the value of a unique identifier. Do not include spaces in the value."@en ;
  rdfs:label "has identifier value"@en .

### https://saref.etsi.org/saref4tser/hasRequestorValue
ats:hasRequestorValue a owl:DatatypeProperty ;
  rdfs:domain ats:hasRequestor ; 
  rdfs:range xsd:string ;
  rdfs:comment "A relationship that defines the identifier of a person making a data request or owning a project. Usually a foaf:maker value. Do not include spaces in the value."@en ;
  rdfs:label "has requestor value"@en .

### https://saref.etsi.org/saref4tser/hasProgression
ats:hasProgression a owl:DatatypeProperty ;
  rdfs:domain ats:AlignedSequence ; 
  rdfs:range xsd:boolean ;
  rdfs:comment "A property indicating whether an AlignedSequence contains every time-index from start to finish."@en ;
  rdfs:label "has progression"@en .

### https://saref.etsi.org/saref4tser/isMultivariate
ats:isMultivariate a owl:DatatypeProperty ;
  rdfs:domain ats:TimeseriesNodeSet ; 
  rdfs:range xsd:boolean ;
  rdfs:comment "Declares whether a TimeseriesNodeSet contains more than one AlignedSequence {true or false}."@en ; 
  rdfs:label "is multivariate"@en .

### https://saref.etsi.org/saref4tser/isEmpty
ats:isEmpty a owl:DatatypeProperty ;
  rdfs:domain ats:AlignedSequence ; 
  rdfs:range xsd:boolean ;
  rdfs:comment "Declares whether an AlignedSequence has any non-null values or not {true or false}."@en ;
  rdfs:label "is empty"@en .

### https://saref.etsi.org/saref4tser/hasCardinality
ats:hasCardinality a owl:DatatypeProperty ;
  rdfs:domain ats:AlignedSequence ; 
  rdfs:range xsd:nonNegativeInteger ;
  rdfs:comment "A property indicating how many non-null values are currently present in an AlignedSequence."@en ;
  rdfs:label "has cardinality"@en .

### https://saref.etsi.org/saref4tser/hasAccuracy
ats:hasAccuracy a owl:DatatypeProperty ;
  rdfs:domain ats:Sequence ; 
  rdfs:range ssn-system:Accuracy ;
  rdfs:isDefinedBy "http://www.w3.org/ns/ssn/systems/Accuracy" ;
  rdfs:comment "Accuracy refers to the closeness of agreement between the value of an Observation and the true value of the observed ObservableProperty."@en ;
  rdfs:label "has accuracy"@en .

### https://saref.etsi.org/saref4tser/hasPrecision
ats:hasPrecision a owl:DatatypeProperty ;
  rdfs:domain ats:Sequence ; 
  rdfs:range ssn-system:Precision ;
  rdfs:isDefinedBy "http://www.w3.org/ns/ssn/systems/Precision" ;
  rdfs:comment "Precision refers to the closeness of agreement between replicate Observations on an unchanged or similar quality value."@en ;
  rdfs:label "has precision"@en .

### https://saref.etsi.org/saref4tser/hasResolution
ats:hasResolution a owl:DatatypeProperty ;
  rdfs:domain ats:Sequence ; 
  rdfs:range ssn-system:Resolution ;
  rdfs:isDefinedBy "http://www.w3.org/ns/ssn/systems/Resolution" ;
  rdfs:comment "Resolution refers to the smallest difference in the value of an ObservableProperty."@en ;
  rdfs:label "has resolution"@en .

