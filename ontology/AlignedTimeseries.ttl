#################################################################
#    ComputationGraph Class
#################################################################

### https://saref.etsi.org/saref4tser/ComputationGraph   
ats:ComputationGraph a owl:Class ;
  rdfs:subClassOf  [ a owl:Restriction ;
    owl:onProperty ats:hasIdentifier ;
    owl:someValuesFrom ats:Identifier 
  ] ,  
  [ a owl:Restriction ;
    owl:onProperty ats:hasPublicDatasetName ;
    owl:someValuesFrom ats:Identifier
  ] ,
  [ a owl:Restriction ;
    owl:onProperty ats:hasDeviceName ;
    owl:someValuesFrom ats:Identifier
  ] ,
  [ a owl:Restriction ;
    owl:onProperty ats:hasAlignedSequence ;
    owl:someValuesFrom ats:AlignedSequence
  ] ,
  [ a owl:Restriction ;
    owl:onProperty saref:isMeasuredIn ;
    owl:someValuesFrom saref:UnitOfMeasure
  ] ,
  [ a owl:Restriction ;
    owl:onProperty ats:hasSequenceFormula ;
    owl:someValuesFrom ats:MathematicalStatement
  ] ,
  [ a owl:Restriction ;
    owl:onProperty ats:hasStationarity 
  ] ,
  [ a owl:Restriction ;
    owl:onProperty ats:hasSeasonality 
  ] ,
  [ a owl:Restriction ;
    owl:onProperty ats:hasAutoCorrelationOrder 
  ] ,
  [ a owl:Restriction ;
    owl:onProperty ats:hasCovarianceValue 
  ] ,
  [ a owl:Restriction ;
    owl:onProperty ats:hasSamplingInterval 
  ] ,
  [ a owl:Restriction ;
    owl:onProperty ats:hasMeasurementUncertaintyPercentage 
  ] ,
  [ a owl:Restriction ;
    owl:onProperty ats:hasTimeseriesModel ;
    owl:someValuesFrom ats:AlignedTimeseriesModel
  ] ,
  [ a owl:Restriction ;
    owl:onProperty ats:hasForecastingModel ;
    owl:someValuesFrom ats:ForecastingModel
  ] ,
  [ a owl:Restriction ;
    owl:onProperty ats:hasErrorModel ;
    owl:someValuesFrom ats:ErrorModel
  ] ,
  [ a owl:Restriction ;
    owl:onProperty ats:hasRecurrenceRelation ;
    owl:someValuesFrom ats:MathematicalStatement
  ] ,
  [ a owl:Restriction ;
    owl:onProperty ats:hasProbabilityDistribution ;
    owl:someValuesFrom ats:MathematicalStatement
  ] ,
  [ a owl:Restriction ;
    owl:onProperty ats:hasUpperBoundingCurve ;
    owl:someValuesFrom ats:MathematicalStatement
  ] ,
  [ a owl:Restriction ;
    owl:onProperty ats:hasLowerBoundingCurve ;
    owl:someValuesFrom ats:MathematicalStatement
  ] ,
  [ a owl:Restriction ;
    owl:onProperty ats:hasConvergenceLimitValue 
  ] ;
  rdfs:comment "A ComputationGraph is a directed graph whose nodes correspond to computable measures of interoperability."@en ;
  rdfs:label "ComputationGraph"@en .

### https://saref.etsi.org/saref4tser/hasAutoCorrelationOrder
ats:hasAutoCorrelationOrder a owl:DatatypeProperty ;
  rdfs:comment "Declares the value of the autocorrelation order. The value is expected to be a small integer value, often 1."@en ;
  rdfs:label "has autocorrelation order"@en ;
  rdfs:domain ats:AlignedSequence ; 
  rdfs:range xsd:integer .

### https://saref.etsi.org/saref4tser/hasSamplingInterval
ats:hasSamplingInterval a owl:DatatypeProperty ;
  rdfs:comment "Declares the designed sampling interval of a time series in seconds. The value is expected to be an integer."@en ;
  rdfs:label "has sampling interval"@en ;
  rdfs:domain ats:ComputationGraph ; 
  rdfs:range xsd:integer .

### https://saref.etsi.org/saref4tser/hasCovarianceValue
ats:hasCovarianceValue a owl:DatatypeProperty ;
  rdfs:comment "Declares the covariance value of a time series. The value is expected to be a decimal value. The specific formulation depends upon the type of time series model."@en ;
  rdfs:label "has covariance value"@en ;
  rdfs:domain ats:AlignedSequence ; 
  rdfs:range xsd:decimal .

### https://saref.etsi.org/saref4tser/hasMeasurementUncertaintyPercentage
ats:hasMeasurementUncertaintyPercentage a owl:DatatypeProperty ;
  rdfs:comment "Declares the average value between measurement reading and its uncertainty using a device`s Accuracy and Precision values. A measurement is considered `valid` if the measurement is both precise and accurate. The value is expected to be an decimal value expressed as a percent."@en ;
  rdfs:label "has measurement uncertainty percentage"@en ;
  rdfs:domain ats:AlignedSequence ; 
  rdfs:range xsd:decimal .

### https://saref.etsi.org/saref4tser/hasConvergenceLimitValue
ats:hasConvergenceLimitValue a owl:DatatypeProperty ;
  rdfs:comment "Declares the convergence limit value of an AlignedSequence."@en ;
  rdfs:label "has convergence limit value"@en ;
  rdfs:domain ats:AlignedSequence ; 
  rdfs:range xsd:decimal .

#################################################################
#    Classes
#################################################################

### https://saref.etsi.org/saref4tser/DataQueryRequest
ats:DataQueryRequest a owl:Class ;
  rdfs:subClassOf [ a owl:Restriction ;
    owl:onProperty ats:hasIdentifier ; 
    owl:someValuesFrom ats:Identifier 
  ] ;
  rdfs:subClassOf [ a owl:Restriction ;
    owl:onProperty ats:hasRequestor ;   # usually foaf:maker
    owl:someValuesFrom xsd:string
  ] ;
  rdfs:subClassOf [ a owl:Restriction ;
    owl:onProperty ats:hasProjectIri ; # "https://mines.ontology.datasets.fr/iotdata/root.homec.weather.HomeC", 
    owl:someValuesFrom xsd:string
  ] ;
  rdfs:subClassOf [ a owl:Restriction ;
    owl:onProperty ats:hasSelectedDataset ; # "root.homec.weather.HomeC"
    owl:someValuesFrom xsd:string
  ] ;
  rdfs:subClassOf [ a owl:Restriction ;
    owl:onProperty ats:hasPreferredSchema ; # "saref|oneM2M"
    owl:someValuesFrom xsd:string
  ] ;
  rdfs:subClassOf [ a owl:Restriction ;
    owl:onProperty ats:hasPreferredService ;  "DCAT-AT"
    owl:someValuesFrom xsd:string
  ] ;
  rdfs:subClassOf [ a owl:Restriction ;
    owl:onProperty ats:hasPreferedFormat ; # "csv"
    owl:someValuesFrom xsd:string
  ] ;
  rdfs:subClassOf [ a owl:Restriction ;
    owl:onProperty ats:hasRandomSelect ; # false
    owl:someValuesFrom xsd:boolean
  ] ;
  rdfs:subClassOf [ a owl:Restriction ;
    owl:onProperty ats:hasContinuousLoop ; # false
    owl:someValuesFrom xsd:boolean
  ] ;
  rdfs:subClassOf [ a owl:Restriction ;
    owl:onProperty ats:hasFilterFunction ;  # AlignedSequenceFunction-InterpolateMissingValues
    owl:someValuesFrom ats:AlignedSequenceFunction
  ] ;
  rdfs:subClassOf [ a owl:Restriction ;
    owl:onProperty ats:hasStartDateTime ; # "2016-01-01T05:00:00.000Z"
    owl:someValuesFrom xsd:dateTime
  ] ;
  rdfs:subClassOf [ a owl:Restriction ;
    owl:onProperty ats:hasEndDateTime ; # "2016-01-07T00:58:29.000Z" 
    owl:someValuesFrom xsd:dateTime
  ] ;
  rdfs:subClassOf [ a owl:Restriction ;
    owl:onProperty ats:hasMultiplier ; # 100
    owl:someValuesFrom xsd:integer
  ] ;
  rdfs:subClassOf [ a owl:Restriction ;
    owl:onProperty ats:hasLimit ; # 100
    owl:someValuesFrom xsd:integer
  ] ;
  rdfs:subClassOf [ a owl:Restriction ;
    owl:onProperty ats:hasDateTimeField ; # "Time1"
    owl:someValuesFrom xsd:string
  ] ;
  rdfs:subClassOf [ a owl:Restriction ;
    owl:onProperty ats:hasSelectedTimeseries ; # "*"
    owl:someValuesFrom xsd:string
  ] ;
  rdfs:comment "A DataQueryRequest specifies which IoT data is to be selected."@en ;
  rdfs:label "DataQueryRequest"@en .

### https://saref.etsi.org/saref4tser/Comparable
ats:Comparable a owl:Class ;
  rdfs:subClassOf saref:Function ,
  [ a owl:Restriction ;
    owl:onProperty ats:hasIdentifier ;
    owl:someValuesFrom ats:Identifier ;
    owl:maxCardinality "1"^^xsd:nonNegativeInteger
  ] ;
  rdfs:comment "A relationship indicating that an AlignedSequence is comparable to another. Comparable indicates AlignedSequence interoperability."@en ;
  rdfs:label "Comparable"@en .

### https://saref.etsi.org/saref4tser/Composable
ats:Composable a owl:Class ;
  rdfs:subClassOf saref:Function ,
  [ a owl:Restriction ;
    owl:onProperty ats:hasIdentifier ;
    owl:someValuesFrom ats:Identifier ;
    owl:maxCardinality "1"^^xsd:nonNegativeInteger
  ] ;
  rdfs:comment "A relationship indicating that an AlignedSequence is composable with another. Composable indicates AlignedSequence interoperability."@en ;
  rdfs:label "Composable"@en .

### https://saref.etsi.org/saref4tser/Formulation
ats:Formulation a owl:Class ;
  rdfs:subClassOf saref:Function ;
  rdfs:comment "A representation of a mathematical function, either symbolic or parameterized."@en ;
  rdfs:label "Formulation"@en .

### https://saref.etsi.org/saref4tser/MathematicalStatement
ats:MathematicalStatement a owl:Class ;
  rdfs:subClassOf saref:Function ,
  [ a owl:Restriction ;
    owl:onProperty ats:hasIdentifier ;
    owl:someValuesFrom ats:Identifier ;
    owl:maxCardinality "1"^^xsd:nonNegativeInteger
  ] ,
  [ a owl:Restriction ;
    owl:onProperty ats:hasSymbolicFormulation ; 
    owl:someValuesFrom ats:Formulation
  ] , 
  [ a owl:Restriction ;
    owl:onProperty ats:hasParametricFormulation ; 
    owl:someValuesFrom ats:Formulation
  ] , 
  [ a owl:Restriction ;
    owl:onProperty ats:isMathematicalStatementOf ;
    owl:someValuesFrom ats:GenericModel
  ] ;
  rdfs:comment "A MathematicalStatement represents a mathematical expression in both symbolic and parametric formulations."@en ;
  rdfs:label "MathematicalStatement"@en .

### https://saref.etsi.org/saref4tser/MeasuringFunction
ats:MeasuringFunction a owl:Class ;
  rdfs:subClassOf saref:Function ;
  rdfs:comment "A function that makes a measurement."@en ;
  rdfs:label "MeasuringFunction"@en .

### https://saref.etsi.org/saref4tser/GenericModel
ats:GenericModel a owl:Class ;
  rdfs:subClassOf [ a owl:Restriction ;
    owl:onProperty ats:hasMathematicalStatement ;
    owl:someValuesFrom ats:MathematicalStatement
  ] ;
  rdfs:comment "A GenericModel indicates a time series model has not been fully specified. GenericModels are not comparable to one another and should be disjoint."@en ;
  rdfs:label "GenericModel"@en .

### https://saref.etsi.org/saref4tser/Sequence 
ats:Sequence a owl:Class ;
  rdfs:subClassOf saref:Command ,
  [ a owl:Restriction ;
    owl:onProperty ats:hasSequenceDatatype 
  ] ,
  [ a owl:Restriction ;
    owl:onProperty ats:hasAccuracy ;
    owl:someValuesFrom saref:Property 
  ] ,
  [ a owl:Restriction ;
    owl:onProperty ats:hasPrecision ;
    owl:someValuesFrom saref:Property
  ] ,
  [ a owl:Restriction ;
    owl:onProperty ats:hasResolution ;
    owl:someValuesFrom saref:Property
  ] ,
  [ a owl:Restriction ;
    owl:onProperty ats:hasIdentifier ;
    owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
    owl:someValuesFrom ats:Identifier 
  ] ,
  [ a owl:Restriction ;
    owl:onProperty saref:isMeasuredIn ;
    owl:someValuesFrom saref:UnitOfMeasure
  ] ,
  [ a owl:Restriction ;
    owl:onProperty time:hasBeginning ;
    owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
    owl:someValuesFrom time:TemporalEntity
  ] ,
  [ a owl:Restriction ;
    owl:onProperty time:hasEnd ;
    owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
    owl:someValuesFrom time:TemporalEntity
  ] ,
  [ a owl:Restriction ; 
    owl:onProperty time:Interval ;  
    owl:cardinality "1"^^xsd:nonNegativeInteger ;
    owl:someValuesFrom time:TemporalEntity
  ] ,
  [ a owl:Restriction ;
    owl:onProperty ats:isEmpty ; 
    owl:someValuesFrom xsd:boolean
  ] ,
  [ a owl:Restriction ;
    owl:onProperty ats:hasCardinality ; 
    owl:someValuesFrom xsd:integer
  ] ;
  rdfs:comment "A Sequence is a value-ordered collection of the same kind of comparable entity. Sequences are described by an explicit formula or function, a recurrence relation, or a table of values."@en ;
  rdfs:label "Sequence"@en .

### https://saref.etsi.org/saref4tser/AlignedTimeseriesModel
ats:AlignedTimeseriesModel a owl:Class ;
  rdfs:subClassOf ats:GenericModel , 
    [ a owl:Restriction ;
     owl:onProperty ats:hasTimeseriesModel ;
     owl:cardinality "1"^^xsd:nonNegativeInteger ;
     owl:someValuesFrom ats:MathematicalStatement 
    ] , 
    [ a owl:Restriction ;
     owl:onProperty ats:hasTrendComponent ;
     owl:someValuesFrom ats:MathematicalStatement 
    ] , 
    [ a owl:Restriction ;
     owl:onProperty ats:hasSeasonalComponent ;
     owl:someValuesFrom ats:MathematicalStatement
    ] , 
    [ a owl:Restriction ;
     owl:onProperty ats:hasIrregularComponent ;
     owl:someValuesFrom ats:MathematicalStatement
    ] , 
    [ a owl:Restriction ;
     owl:onProperty ats:hasCyclicComponent ;
     owl:someValuesFrom ats:MathematicalStatement
    ] , 
    [ a owl:Restriction ;
     owl:onProperty ats:hasAutoCorrelation ;
     owl:someValuesFrom ats:MathematicalStatement
    ] , 
    [ a owl:Restriction ;
     owl:onProperty ats:hasCovarianceFunction ;
     owl:someValuesFrom ats:MathematicalStatement
    ] , 
    [ a owl:Restriction ;
     owl:onProperty ats:hasStationarity ;
     owl:someValuesFrom ats:MathematicalStatement
    ] , 
    [ a owl:Restriction ;
     owl:onProperty ats:hasSeasonality ;
     owl:someValuesFrom ats:MathematicalStatement
    ] ;
  rdfs:comment "An AlignedTimeseriesModel specifies the model of a time-indexed AlignedSequence. The model includes a trend component, a seasonal component, a cyclic component, and an irregular component. The model is characterized by its properties of auto-correlation, covariance, stationarity, and seasonality."@en ;
  rdfs:label "AlignedTimeseriesModel"@en .

### https://saref.etsi.org/saref4tser/ForecastingModel
ats:ForecastingModel a owl:Class ;
  rdfs:subClassOf ats:GenericModel , 
    [ a owl:Restriction ;
     owl:onProperty ats:hasForecastingModel ;
     owl:someValuesFrom ats:MathematicalStatement
    ] ;
  rdfs:comment "A ForecastingModel evaluates trends and future measurement values. Well-known subclasses include Autoregressive, Moving average, Autoregressive moving average, Autoregressive integrated moving average, Seasonal autoregressive integrated moving average, Vector autoregressive, Vector error correction, Exponential smoothing, etc."@en ;
  rdfs:label "ForecastingModel"@en .

### https://saref.etsi.org/saref4tser/ErrorModel
ats:ErrorModel a owl:Class ;
  rdfs:subClassOf ats:GenericModel ,
    [ a owl:Restriction ;
     owl:onProperty ats:hasErrorModel ;
     owl:someValuesFrom ats:MathematicalStatement
    ] ;
  rdfs:comment "An ErrorModel describes the distribution of errors produced during measurement sampling, often Gaussian."@en ;
  rdfs:label "ErrorModel"@en .

### https://saref.etsi.org/saref4tser/WebService
ats:WebService a owl:Class ;
  rdfs:subClassOf saref:Service ,
   [ a owl:Restriction ;
    owl:onProperty ats:hasIdentifier ;
    owl:someValuesFrom ats:Identifier ;
    owl:maxCardinality "1"^^xsd:nonNegativeInteger
   ] ;
  rdfs:comment "A type of Service that exposes a declared Application Programming Interface (API) and can be composed with other services."@en ;
  rdfs:label "WebService"@en .

### https://saref.etsi.org/saref4tser/VirtualDevice
ats:VirtualDevice a owl:Class ;
  rdfs:subClassOf saref:Device ,
    [ a owl:Restriction ;
     owl:onProperty ats:hasWebService ; 
     owl:someValuesFrom saref:Service
    ] ;
  rdfs:comment "A Device that produces a synthetic AlignedSequence."@en ;
  rdfs:label "VirtualDevice"@en .

### https://saref.etsi.org/saref4tser/DataAccessWebService
ats:DataAccessWebService a owl:Class ;
  rdfs:subClassOf ats:WebService ,
    [ a owl:Restriction ;
     owl:onProperty ats:AlignedSequenceFunction ;  
     owl:someValuesFrom saref:Function
    ] ;
  rdfs:comment "A WebService that fetches all the measurement data at one time Instant, or for an Interval, for a TimeSeriesNodeSet."@en ;
  rdfs:label "DataAccessWebService"@en .

### https://saref.etsi.org/saref4tser/Clock
ats:Clock a owl:Class ; 
  rdfs:subClassOf saref:Device ;
  rdfs:comment "A Clock is a device responsible for maintaining uniform UTC time at a specified Resolution. A Clock often generates an acccessible heartbeat event when attached to a single Network or multiple Devices. A Clock is always On. A Clock provides an accessible Reset command to account for device drift. The Interval for a Network Clock can be set by an AlignedSequence."@en ;
  rdfs:label "Clock"@en .

### https://saref.etsi.org/saref4tser/ProbabilityDistribution 
ats:ProbabilityDistribution a owl:Class ;
  rdfs:subClassOf ats:GenericModel ,
    [ a owl:Restriction ;
     owl:onProperty ats:hasAlignedSequenceFunction ;
     owl:someValuesFrom ats:MathematicalStatement
    ] ;
  rdfs:comment "The ProbabilityDistribution class specifies a discrete or continuous probability distribution that best characterizes a sequence of measurement samples over a defined period of time. Well-known discrete distributions include Uniform, Bernoulli, Binomial, and Poisson."@en ;
  rdfs:label "ProbabilityDistribution"@en .

### https://saref.etsi.org/saref4tser/AlignedSequenceEvent
ats:AlignedSequenceEvent a owl:Class ;
  rdfs:subClassOf saref:EventFunction ,
   [ a owl:Restriction ;
    owl:onProperty ats:hasIdentifier ;
    owl:someValuesFrom ats:Identifier ;
    owl:maxCardinality "1"^^xsd:nonNegativeInteger
   ] ,
   [ a owl:Restriction ;
     owl:onProperty saref:hasTimestamp ;
     owl:someValuesFrom xsd:dateTime
   ] ;
  rdfs:comment "An AlignedSequenceEvent is either emitted by or listened for by an AlignedSequence; for example, a value threshold has been crossed."@en ;
  rdfs:label "AlignedSequenceEvent"@en .

### https://saref.etsi.org/saref4tser/AlignedSequenceFunction
ats:AlignedSequenceFunction a owl:Class ;
  rdfs:subClassOf saref:Function ,
   [ a owl:Restriction ;
    owl:onProperty ats:hasIdentifier ;
    owl:someValuesFrom ats:Identifier ;
    owl:maxCardinality "1"^^xsd:nonNegativeInteger
   ] ;
  rdfs:comment "An AlignedSequenceFunction operates over an immutable AlignedSequence to produce a result."@en ;
  rdfs:label "AlignedSequenceFunction"@en .

### https://saref.etsi.org/saref4tser/TimeseriesNodeSet 
ats:TimeseriesNodeSet a owl:Class ;
  rdfs:subClassOf [ a owl:Restriction ;
    owl:onProperty ats:hasProcedure ;
    owl:someValuesFrom ats:AlignedSequenceFunction
   ] ,
   [ a owl:Restriction ;
    owl:onProperty ats:hasMember ;
    owl:someValuesFrom ats:AlignedSequence 
   ] ,
   [ a owl:Restriction ;
    owl:onProperty ats:hasDataAccessWebService ;
    owl:someValuesFrom ats:DataAccessWebService 
   ] ,
   [ a owl:Restriction ;
     owl:onProperty ats:isMultivariate ;
     owl:someValuesFrom xsd:boolean
   ] ;
  rdfs:comment "A collection of several time-aligned AlignedSequences."@en ;
  rdfs:label "TimeseriesNodeSet"@en .

### https://saref.etsi.org/saref4tser/AlignedSequence 
ats:AlignedSequence a owl:Class ;
  rdfs:subClassOf ats:Sequence ,
  [ a owl:Restriction ;
    owl:onProperty ats:hasIdentifier ;
    owl:someValuesFrom ats:Identifier ;
    owl:maxCardinality "1"^^xsd:nonNegativeInteger
  ] ,
  [ a owl:Restriction ;
   owl:onProperty saref:observes ;
   owl:someValuesFrom saref:Observation 
  ] ,
  [ a owl:Restriction ;
   owl:onProperty ats:hasIntervalBasedSamplingTime ;
   owl:someValuesFrom ns1:samplingTime
  ] ,
  [ a owl:Restriction ;
   owl:onProperty ats:hasObservationLocation ;
   owl:someValuesFrom wgs84:location
  ] ,
  [ a owl:Restriction ;
    owl:onProperty ats:hasAlignedSequenceFunction ; 
    owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:someValuesFrom ats:AlignedSequenceFunction 
  ] ,
  [ a owl:Restriction ;
